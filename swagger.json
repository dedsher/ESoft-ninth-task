{
  "openapi": "3.0.0",
  "info": {
    "title": "Express API for Users",
    "version": "1.0.0",
    "description": "This is a REST API application made with Express."
  },
  "servers": [
    {
      "url": "http://localhost:3000",
      "description": "Development server"
    }
  ],
  "paths": {
    "/users": {
      "get": {
        "summary": "Получение списка пользователей",
        "responses": {
          "200": {
            "description": "List of users",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "id": {
                        "type": "string",
                        "description": "ID пользователя",
                        "example": "c6bf0e55-db67-49bc-9af6-c900c5da1d4a"
                      },
                      "name": {
                        "type": "string",
                        "description": "Имя пользователя",
                        "example": "Romchik Shamov"
                      },
                      "email": {
                        "type": "string",
                        "description": "Почта пользователя",
                        "example": "bebronyuh@ugar.fun"
                      },
                      "age": {
                        "type": "number",
                        "description": "Количество полных лет пользователя",
                        "example": 4
                      }
                    }
                  }
                }
              }
            }
          },
          "404": {
            "description": "Users not found"
          }
        }
      },
      "post": {
        "summary": "Создание нового пользователя",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "array",
                "items": {
                  "type": "object",
                  "required": [
                    "name",
                    "email",
                    "age"
                  ],
                  "properties": {
                    "name": {
                      "type": "string",
                      "description": "Имя пользователя",
                      "example": "Romchik Shamov"
                    },
                    "email": {
                      "type": "string",
                      "description": "Почта пользователя",
                      "example": "bebronyuh@ugar.fun"
                    },
                    "age": {
                      "type": "number",
                      "description": "Количество полных лет пользователя",
                      "example": 4
                    }
                  }
                }
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Created"
          },
          "400": {
            "description": "Invalid user data"
          }
        }
      }
    },
